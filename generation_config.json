{
    "device": "mps",
    "tokenizer":{
        "vocab_path":"tinystories_tokenizer/vocab.pkl",
        "merges_path":"tinystories_tokenizer/merges.pkl",
        "special_tokens": ["<|endoftext|>"]
    },
    "model": {
        "checkpoint_path":"checkpoints/tinystories_checkpoint.pt",
        "vocab_size": 10000,
        "context_length": 256,
