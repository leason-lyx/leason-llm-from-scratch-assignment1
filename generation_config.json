        "num_layers": 4,
        "num_heads": 16,
        "dff": 1344,
        "rope_theta": 10000.0,
        "dtype": "float32"
    },
    "generation": {
        "max_new_tokens": 500,
        "temperature": 1.0,
        "top_p": 0.9
    }
}